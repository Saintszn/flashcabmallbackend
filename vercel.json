
{
  "version": 2,
  "functions": {
    "server.js": {
      "runtime": "nodejs18.x"
    }
  },
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
    {
      "src": "/socket\\.io/.*",
      "dest": "server.js"
    },

      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }
  